{
    "name": "stuchilde/submail-php-sdk",
    "description": "赛邮云通信submail php'sdk",
    "keywords": [
        "submail",
        "php",
        "sdk"
    ],
    "homepage": "https://www.mysubmail.com",
    "license": "MIT",
    "authors": [
        {
            "name": "stuchilde",
            "email": "stuchilde@outlook.com",
            "homepage": "https://icoder.tech/",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.6.0",
        "guzzlehttp/guzzle": "^6.3@dev"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "Submail\\SDK\\": "src"
        }
    },
    "type" : "library",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Submail\\SDK\\SubmailServiceProvider"
            ],
            "aliases": {
                "Debugbar": "Submail\\SDK\\Facade"
            }
        }
    },
    "minimum-stability": "stable"
}
